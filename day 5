
version 1:
#include <stdio.h>

struct fract
{

    int num,den;
};
struct fract input()
{
    struct fract i;
    printf("\nEnter the fraction(numerator/denominator):\n");
    scanf("%d%d",&i.num,&i.den);
    printf("entered is %d/%d",i.num,i.den);
}
int main()
{
    struct fract i;
    i=input();
    return 0;
}
